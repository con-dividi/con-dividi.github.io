{
  "name": "@onwidget/astrowind",
  "description": "A template to make your website using Astro + Tailwind CSS.",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "optimize-images": "./scripts/optimize-images.sh",
    "build": "yarn optimize-images && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "yarn format && eslint --ext .js,.astro src",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "lint": "eslint --ext .js,.astro src",
    "subfont": "subfont -ir --no-fallbacks --silent --root dist"
  },
  "devDependencies": {
    "@astrojs/mdx": "^1.1.5",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrolib/analytics": "^0.4.2",
    "@astrolib/seo": "^0.6.0",
    "@fontsource/inter": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.16.0",
    "astro-icon": "^0.8.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.1",
    "limax": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "reading-time": "^1.5.0",
    "subfont": "^6.12.2",
    "typescript": "^5.3.2",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6",
    "yaml-schema-validator": "^1.2.3"
  },
  "engines": {
    "node": "^14.15.0 || >=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@astrojs/svelte": "^4.0.4",
    "@astrojs/tailwind": "^5.0.2",
    "astro": "3.6.4",
    "flowbite": "^2.1.1",
    "svelte": "^4.2.8",
    "tailwindcss": "^3.0.24"
  }
}
